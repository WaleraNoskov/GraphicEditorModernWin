<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"

    <Style TargetType="local2:InstrumentViewModel" xmlns:local2="using:GraphicEditorModernWin.Feature.Drawing.ViewModel">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="local2:InstrumentViewModel">
                    <Border
                        Background="{TemplateBinding Background}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}">
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
	xmlns:local="using:GraphicEditorModernWin" />
